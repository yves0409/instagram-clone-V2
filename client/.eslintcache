ard login-card\">\n        <h2>Login</h2>\n        <input\n          type=\"text\"\n          placeholder=\"email\"\n          value={email}\n          onChange={(e) => setEmail(e.target.value)}\n        />\n        <input\n          type=\"password\"\n          placeholder=\"password\"\n          value={password}\n          onChange={(e) => setPassword(e.target.value)}\n        />\n\n        <button\n          className=\"btn waves-effect waves-light #82b1ff blue accent-1\n\"\n          onClick={() => postData()}\n        >\n          Login\n        </button>\n        <p>\n          <Link to=\"/signup\">Don't have an account yet? Signup for free !</Link>\n        </p>\n      </div>\n    </div>\n  );\n};\n\nexport default Login;\n","/Users/yves/Sites/MERN + backend projects/instaclone/client/src/components/views/Profile.js",["60"],"/Users/yves/Sites/MERN + backend projects/instaclone/client/src/components/views/Signup.js",["61","62"],"import React, { useState } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport M from \"materialize-css\";\n\nconst Signup = () => {\n  const history = useHistory();\n\n  const [name, setName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n\n  const postData = () => {\n    //Validate email regex\n    if (\n      !/^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test(\n        email\n      )\n    ) {\n      M.toast({\n        html: \"Invalid email\",\n        classes: \"#e57373 red lighten-2\",\n      });\n      return;\n    }\n\n    //request to backend\n    fetch(\"/signup\", {\n      method: \"post\",\n      headers: {\n        \"content-Type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        name,\n        password,\n        email,\n      }),\n    })\n      .then((res) => res.json())\n      .t